import java.util.*; 
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int ans=0;
		int placeValue=1;
		while(n>0) {
			int remain=n%10;
			ans+=remain*placeValue;
				placeValue*=2;
				n=n/10;
		}
		System.out.print(ans);


	}
}
