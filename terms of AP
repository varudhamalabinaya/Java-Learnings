import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        sc.close();

        int count = 0; 
        for (int i = 1; count < N; i++) {
            int ans = 3 * i + 2;
            if (ans % 4 != 0) { 
                System.out.print(ans + " ");
                count++; 
            }
        }
    }
}
